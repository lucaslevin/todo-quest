{
	"name": "todo-quest",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bun --hot src/index.ts",
		"start": "NODE_ENV=production bun src/index.ts",
		"build": "bun run build.ts",
		"lint": "bunx biome lint",
		"shadcn:add": "bunx --bun shadcn@latest add --all",
		"auth:schema": "bunx --bun @better-auth/cli@latest generate --output src/db/schema/auth.ts --yes",
		"db:generate": "bunx --bun drizzle-kit generate",
		"db:migrate": "bunx --bun drizzle-kit migrate",
		"db:push": "bunx --bun drizzle-kit push"
	},
	"dependencies": {
		"@base-ui/react": "^1.1.0",
		"@better-auth/stripe": "^1.4.18",
		"@dicebear/collection": "^9.3.1",
		"@dicebear/core": "^9.3.1",
		"@fontsource-variable/cinzel": "^5.2.8",
		"@fontsource-variable/inter": "^5.2.8",
		"@fontsource/cinzel-decorative": "^5.2.8",
		"@hookform/resolvers": "^5.2.2",
		"@mantine/hooks": "^8.3.13",
		"@mesh-gradient/react": "^1.5.0",
		"@orpc/client": "^1.13.4",
		"@orpc/server": "^1.13.4",
		"@phosphor-icons/react": "^2.1.10",
		"better-auth": "^1.4.18",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"drizzle-orm": "^0.45.1",
		"embla-carousel-react": "^8.6.0",
		"input-otp": "^1.4.2",
		"motion": "^12.29.2",
		"nanoid": "^5.1.6",
		"next-themes": "^0.4.6",
		"nuqs": "^2.8.6",
		"postgres": "^3.4.8",
		"radix-ui": "^1.4.3",
		"react": "^19",
		"react-day-picker": "^9.13.0",
		"react-dom": "^19",
		"react-hook-form": "^7.71.1",
		"react-resizable-panels": "^4.5.7",
		"recharts": "2.15.4",
		"sonner": "^2.0.7",
		"stripe": "^20.3.0",
		"swr": "^2.3.8",
		"tailwind-merge": "^3.4.0",
		"vaul": "^1.1.2",
		"wouter": "^3.9.0",
		"zod": "^4.3.6",
		"zustand": "^5.0.10"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/bun": "latest",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"bun-plugin-tailwind": "^0.1.2",
		"drizzle-kit": "^0.31.8",
		"shadcn": "^3.7.0",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0"
	}
}
